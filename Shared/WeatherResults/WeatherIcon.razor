    src="@(Url)"
    alt="@(Description)"
    title="@(Description)"
    class="inline object-contain -m-3 text-sm pointer-events-none select-none size-40"
>

@code {
    [Parameter] public string Url {get;set;} = null!;
    [Parameter] public string Description {get;set;} = null!;
}
